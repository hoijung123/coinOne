<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="coinone.tran.dao.DbMapper.Ticker">
	<insert id="register">
		INSERT INTO `coinone`.`t_ticker`
		(`currency`,
		`timestamp`,
		`high`,
		`low`,
		`last`,
		`first`,
		`volume`)
		VALUES
		(#{currency},
		#{timestamp},
		#{high},
		#{low},
		#{last},
		#{first},
		#{volume})
	</insert>

	<!-- /* select dual */ -->
	<select id="getList" resultType="coinone.tran.vo.TickerDtlVO">
		SELECT `t_ticker`.`currency`,
		`t_ticker`.`timestamp`,
		`t_ticker`.`high`,
		`t_ticker`.`low`,
		`t_ticker`.`last`,
		`t_ticker`.`first`,
		`t_ticker`.`volume`
		FROM `coinone`.`t_ticker`
	</select>
	
	<select id="get" resultType="coinone.tran.vo.TickerDtlVO">
		SELECT `t_ticker`.`currency`,
		`t_ticker`.`timestamp`,
		`t_ticker`.`high`,
		`t_ticker`.`low`,
		`t_ticker`.`last`,
		`t_ticker`.`first`,
		`t_ticker`.`volume`
		FROM `coinone`.`t_ticker`
	   where currency = #{currency}
	   and timestamp = #{timestamp}
	</select>	

</mapper>